type File implements Node {
  contentType: String!
  createdAt: DateTime!
  id: ID! @isUnique
  name: String!
  secret: String! @isUnique
  size: Int!
  updatedAt: DateTime!
  url: String! @isUnique
  resizedCopies: [String!]
}

type User implements Node {
  createdAt: DateTime!
  updatedAt: DateTime!
  id: ID! @isUnique
  firstName: String!
  lastName: String!
  dateOfBirth: DateTime!
  gender: Gender!
}

type Detail implements Node {
  createdAt: DateTime!
  updatedAt: DateTime!
  id: ID! @isUnique
  destination: Destinations!
  duration: Int!
  price: Int!
  currency: Currencies!
}

enum Currencies {
  USD
  EUR
  UAH
}

enum Destinations {
  Japan
  USA
  Ukraine
  England
  Australia
  Canada
}

enum Gender {
  Male
  Female
}